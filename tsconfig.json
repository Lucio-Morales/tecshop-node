{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "types": ["node", "express"]
  },
  "include": ["src/**/*", "types"],
  "exclude": ["node_modules", "dist"]
}
